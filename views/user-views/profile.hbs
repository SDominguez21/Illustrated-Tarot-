<form action="/spreads" class='newReadingForm'><button>New Reading</button></form>

<div class="past-spreads">
  {{#each pastSpread}}
  <div class="individual">
    <div class="date">
      {{this.created_at}}
    </div>
    <div class="type">
      {{this.type}}
    </div>
    <div class="see-all">
      <form action="/spreads/{{this._id}}"><button class="btn">Spread</button></form>
    </div>
  </div>
  {{/each}}
</div>